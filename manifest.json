{
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.4.0",
      "content_scripts":[{
        "matches": [
            "https://www.google.com/calendar/*",
            "https://calendar.google.com/calendar/*"
        ],
        "js": [ "events.user.js" ],
        "run_at": "document_end"
    }],
  "permissions": [
        "https://www.google.com/calendar/*",
        "https://calendar.google.com/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Toggle"
  },
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2
}
